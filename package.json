{
	"name": "gatsby-starter-default-eslint-scss",
	"private": false,
	"description": "An extension of gatsby-stater-default with scss and eslint integration",
	"version": "0.1.0",
	"author": "Jesse George Singleton <jessesingleton441@gmail.com>",
	"dependencies": {
		"gatsby": "^2.13.67",
		"gatsby-image": "^2.2.8",
		"gatsby-plugin-manifest": "^2.2.5",
		"gatsby-plugin-offline": "^2.2.6",
		"gatsby-plugin-react-helmet": "^3.1.3",
		"gatsby-plugin-sass": "^2.1.8",
		"gatsby-plugin-sharp": "^2.2.12",
		"gatsby-source-filesystem": "^2.1.9",
		"gatsby-transformer-sharp": "^2.2.6",
		"node-sass": "^4.12.0",
		"prop-types": "^15.7.2",
		"react": "^16.9.0",
		"react-dom": "^ 16.9.0",
		"react-helmet": "^5.2.1"
	},
	"devDependencies": {
		"eslint": "^6.2.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.14.3",
		"husky": "^3.0.4",
		"prettier": "^1.18.2"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"lint": "eslint --ext .js src --fix",
		"format": "prettier --write \"**/*.+(js)\"",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run format"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JesseSingleton/gatsby-starter-default-eslint-scss"
	},
	"bugs": {
		"url": "https://github.com/JesseSingleton/gatsby-starter-default-eslint-scss/issues"
	}
}
