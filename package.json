{
  "name": "gatsby-starter-default-eslint-scss",
  "private": false,
  "description": "An extension of gatsby-stater-default with scss and eslint integration",
  "version": "0.1.0",
  "author": "Jesse George Singleton <jessesingleton441@gmail.com>",
  "dependencies": {
    "gatsby": "^2.24.11",
    "gatsby-image": "^2.4.9",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-sass": "^2.1.8",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "husky": "^3.0.4",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JesseSingleton/gatsby-starter-default-eslint-scss"
  },
  "bugs": {
    "url": "https://github.com/JesseSingleton/gatsby-starter-default-eslint-scss/issues"
  }
}
